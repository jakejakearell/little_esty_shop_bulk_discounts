<%= form_with model: @discount, method: method, url: path, local: true do |form| %>
  <%= form.label 'quantity_threshold' %>
  <%= form.text_field :quantity_threshold %><br>
  <%= form.label 'percentage_discount' %>
  <%= form.text_field :percentage_discount %><br>
  <%= form.submit 'Submit' %>
<% end %>
